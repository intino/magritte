defrule type(Box)
package magritte.store.metamodel;

import magritte.Model;
import magritte.editors.Box;

public class $name+firstUppercase extends Box {
    public static final Box box = new $name+firstUppercase();

    private Box$[] includes = {
        $boxName...[,$NL]
    };

    @Override
    protected void doWrite(Model model) {
        for (Box include : includes) include.write(model);
    }
}
endrule
