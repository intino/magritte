def type(Variable & Word & multiple) !type(outDefined) !slot(values) !slot(wordvalues) trigger(parameters)
	List<$type+firstUpperCase> $name+javaValidWord
end

def type(Variable & Word & multiple & OutDefined) !slot(values) !slot(wordvalues) trigger(parameters)
	List<$generatedLanguage+LowerCase.rules.$rule+externalWordClass> $name+firstLowerCase
end

def type(Variable & Word & outDefined) !slot(values) !slot(wordvalues) trigger(parameters)
	$generatedLanguage+LowerCase.rules.$rule+externalWordClass $name+firstLowerCase+javaValidWord
end

def type(Variable & Word) !type(outDefined) !slot(values) !slot(wordvalues) trigger(parameters)
	$qn.$type+firstUpperCase $name+javaValidWord
end

def type(Variable & reactive & multiple) !type(empty) !slot(values) !slot(wordvalues) trigger(parameters)
	tara.magritte.Expression<java.util.List<$type+variableTypeList+reference>> $name+firstLowerCase+javaValidWord
end

def type(Variable & reactive) !type(multiple | empty) !slot(values) !slot(wordvalues) trigger(parameters)
	tara.magritte.Expression<$type+variableTypeList+reference> $name+firstLowerCase+javaValidWord
end

def type(Variable & function) !type(empty) !slot(values) !slot(wordvalues) trigger(parameters)
	$generatedLanguage+LowerCase.functions.$rule+interfaceClass $name+firstLowerCase+javaValidWord
end

def type(Variable & multiple) !type(concept | empty) !slot(values) !slot(wordvalues) trigger(parameters)
	java.util.List<$type+variableTypeList+reference> $name+firstLowerCase+javaValidWord
end

def type(Variable) !type(concept | empty | multiple) !slot(values) !slot(wordvalues) trigger(parameters)
	$type+reference+variableType $name+firstLowerCase+javaValidWord
end

def type(Variable & concept & multiple) !type(empty) !slot(values) trigger(parameters)
	java.util.List<tara.magritte.Concept> $name+firstLowerCase+javaValidWord
end

def type(Variable & concept) !type(multiple | empty) !slot(values) trigger(parameters)
	tara.magritte.Concept $name+firstLowerCase+javaValidWord
