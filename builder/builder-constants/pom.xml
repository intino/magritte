<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0
                             http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>io.intino.tara</groupId>
    <artifactId>builder-constants</artifactId>
    <version>1.0.0</version>

    <properties>
        <maven.compiler.source>1.8</maven.compiler.source>
        <maven.compiler.target>1.8</maven.compiler.target>
    </properties>

    <build>
        <outputDirectory>../../out/production/tara-builder-constants</outputDirectory>
        <testOutputDirectory>../../out/test/tara-builder-constants</testOutputDirectory>
        <directory>../../out/build/tara-builder-constants</directory>
        <sourceDirectory>src</sourceDirectory>

    </build>

    <distributionManagement>
        <repository>
            <id>intino-maven</id>
            <url>https://artifactory.intino.io/artifactory/release-builders</url>
        </repository>
    </distributionManagement>
</project>