def type(Stash)
    dsl $dsl

    $root
end

def type(Case)
    $types[ $name][ > $variables...[;]]
        $components...[$NL]\
end

def type(Type)
    $name
end

def type(Variable)
    $name = $value+format...[ ]
end

def type(String) trigger(format)
    "$value"
end

def type(Word) trigger(format)
    $label
end

def type(Date) trigger(format)
    "$value+Instant"
end

def type(Resource) trigger(format)
    "$id"
end

def type(Reference) trigger(format)
    "$name@$type"
end
